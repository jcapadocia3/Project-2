<header>
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-right ">
        <button type="button" id="logout" class="btn btn-primary logout-button search-button-color">logout</button>
      </div>
    </div>
  </div>
  <h1>v.i.e.w.s.</h1>
  <p class="pun">"etched in tone..."</p>
</header>

<main>
  <div class="input-group search-form">
    <div class="testing">
      <input id="search-input1" class="form-control" type="text" placeholder="search by..." aria-label="Search" />
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="bunchaSearchBtns col text-center">
        <button type="button" id="mural-search-button1" class="btn btn-primary search-button-color">zip code</button>
        <button type="button" id="mural-title-button" class="btn btn-primary search-button-color">title</button>
        <button type="button" id="luckybutton" class="luckybutton btn btn-primary align-center search-button-color">find
          one for me!</button>
      </div>
    </div>
  </div>
  <div class="muralSearch2 main-body container">
    <div class="left-body container">
      <div class="searchMuralInfo container">
        <h2 class="muralInfoTitle">mural information</h2>
        <table class="table-section2">
          <tbody id="tablebody1">
            <tr>
              <td>
              </td>
            </tr>
          </tbody>
        </table>
        <br>
      </div>
    </div>
  </div>
  <div class="main-body container">
    <div class="left-body container">
      <div class="muralInfo container">
        <h2 class="muralInfoTitle">mural image</h2>
      </div>
    </div>
    <div class="right-body container">
      <div class="reviews container">
        <h2 class="reviewsTitle">reviews</h2>

        {{#each murals as |mural|}}
        <div>
          <div>
            <h5>
              <a href="/mural/{{mural.id}}">{{mural.name}}</a>
            </h5>
          </div>
        </div>
        {{/each}}

      </div>
    </div>
  </div>
</main>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.1/umd/popper.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/js/bootstrap.min.js"></script>
<script src="/js/profile.js"></script>
<script src="/js/logout.js"></script>
<script src="/js/user.js"></script>
<script src="/js/lucky.js"></script>
